{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppConfiguration": {
    "AllowUnauthorizedUse": true
  },
  "AWS": {
    "S3": {
      "BucketName": "light-wiki-bucket"
    }
  },
  "ImageSizes": {
    "PatternPattern": "{0}-{1}",
    "Small": 200,
    "Medium": 400,
    "Large": 600,
    "ExtraLarge": 1000
  }
}
